ENDIF
ENDFOR
end

BEGIN
FOR i FROM 0 TO list.lenght-2 step 1 DO
if(text[a] = " ")
b = b+1
ENDIF
ENDFOR
end

BEGIN
FOR i FROM 0 TO list.lenght-2 step 1 DO
if(text[a] = a || text[a] = e || text[a] = i || text[a] = o || text[a] = u )
